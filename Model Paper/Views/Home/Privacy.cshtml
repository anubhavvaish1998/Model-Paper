@{
    ViewData["Title"] = "File Upload Page";
}
<div class="text-center">
    <h1 class="display-4">Upload File</h1>
</div>
<div class="container">
    <div class="mb-3 mt-3">
        <form method="post" enctype="multipart/form-data" asp-action="Upoload" asp-controller="Home">
            <div class="mb-3 mt-3">
                <label class="form-lable">
                    Upload File
                </label>
                <input type="file" name="file" class="form-control" />
            </div>
            <div class="mb-3 mt-3">
               <button type="submit" class="btn btn-primary">Upload</button>
            </div>
        </form>
    </div>
</div>






